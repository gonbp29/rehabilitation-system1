    "@mediapipe/camera_utils": "^0.3.1675466862",
    "@mediapipe/drawing_utils": "^0.3.1675466124",
    "@mediapipe/pose": "^0.5.1675469404",
    "@mui/material": "^7.1.1",
    "@tanstack/react-query": "^5.74.4",
    "@types/bcrypt": "^5.0.2",
    "axios": "^1.9.0",
    "bcrypt": "^6.0.0",
    "concurrently": "^8.0.1",
    "googleapis": "^148.0.0",
    "mysql2": "^3.14.1",
    "react-modal": "^3.16.1",
    "react-router-dom": "^7.6.2",
    "recharts": "^2.15.3",
    "sequelize": "^6.37.7",
    "stylis": "^4.3.6",
    "stylis-plugin-rtl": "^2.1.1"
  },
